{
    "name": "fabpot/silex-skeleton",
    "description": "A pre-configured skeleton for the Silex microframework",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.3.3",
        "silex/silex": "1.1.*",
        
        "twig/twig": ">=1.8.0,<2.0-dev",
        "twig/extensions":"1.*",
        "monolog/monolog": "1.5.*",
        
        "symfony/browser-kit": "2.3.*",
        "symfony/class-loader": "2.3.*",
        "symfony/config": "2.3.*",
        "symfony/console": "2.3.*",
        "symfony/css-selector": "2.3.*",
        "symfony/finder": "2.3.*",
        "symfony/form": "2.3.*",
        "symfony/monolog-bridge": "2.3.*",
        "symfony/process": "2.3.*",
        "symfony/security": "2.3.*",
        "symfony/translation": "2.3.*",
        "symfony/twig-bridge": "2.3.*",
        "symfony/validator": "2.3.*",
        "symfony/doctrine-bridge": "2.3.*",

        "swiftmailer/swiftmailer": ">=4.1.2,<4.2-dev",
        "dflydev/doctrine-orm-service-provider": "1.0.*",
        "yosymfony/config-serviceprovider": "1.1.*",
        "knplabs/console-service-provider": "dev-master",
        "entea/silex-twig-simple-asset-extension": "dev-master",
        "volcanus/csv": "dev-master"
    },
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
